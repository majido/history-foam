{
  "version": "0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "37.0.0.0",
  "name": "FOAM History",
  "description": "A History page replacement",
  "offline_enabled": true,
  "chrome_url_overrides" : {
    "history": "History.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  
  "permissions": [
    "chrome://favicon/*",
    "history"
  ]
}
